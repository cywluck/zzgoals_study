/* pages/search/search.wxss */
@import "../page-template/compage.wxss";
.search-box{width:100%;height:87rpx;background-color: #f5f5f5;position: relative;overflow: hidden;}
.search-box .input1{color:#b2b2b2;width:710rpx;height:55rpx;border:1px solid #f2f2f4;border-radius:16rpx;text-align: center;line-height: 55rpx;background-color: #fff;margin:15rpx auto;}
.search-box .icon-sousuo{color:#b2b2b2;position: absolute;left:40%;top:26rpx;z-index:1000;}
 
.search-box1{width:710rpx;height:87rpx;background-color: #f5f5f5;position: relative;overflow: hidden;padding: 0 20rpx;}
.search-box1 .input1{color:#b2b2b2;width:580rpx;height:55rpx;border:1px solid #f2f2f4;border-radius:16rpx;line-height: 55rpx;background-color: #fff;padding-left:50rpx;margin-top:15rpx;float:left; }
.search-box1 .icon-sousuo{color:#b2b2b2;position: absolute;left:37rpx;top:26rpx;z-index:1000;}
.search-box1 .cancle{float:right;line-height: 87rpx;font-size: 32rpx;color:#4a88ed;}
.box{border-top:1px solid #d6d6db;}
.search-img{margin:100rpx 0;height:200rpx;}
.search-img image{display: block;width:220rpx;height:200rpx;margin:0 auto;}
.search-text{width:100%}
.search-text text:first-child{display: block;text-align: center;font-size: 40rpx;color:#888888;height:80rpx;}
.search-text text:last-child{display: block;text-align: center;font-size: 28rpx;color:#888888;height:50rpx;}
.bton{padding: 0 20rpx;margin-top:50rpx;padding-bottom: 285rpx;border-bottom: 1px solid #e5e5e5;}
.bton button{background-color: #4a88ed!important;}

.container>image{width:750rpx;height:122rpx;display: block;}