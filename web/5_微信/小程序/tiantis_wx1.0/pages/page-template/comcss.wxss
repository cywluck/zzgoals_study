  url('//at.alicdn.com/t/font_vsjn8mzql4n29.woff') format('woff'),
  url('//at.alicdn.com/t/font_vsjn8mzql4n29.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_vsjn8mzql4n29.svg#iconfont') format('svg');
}
